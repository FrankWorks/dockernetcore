{
  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": true,
    "debugType": "portable"
  },
  "dependencies": {
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.0"
    }
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": "dnxcore50"
    }
  },
  "tooling": {
    "defaultNamespace": "VSCodeDebugging"
  },
  "publishOptions": {
    "include": [
      "Dockerfile.debug",
      "Dockerfile",
      "docker-compose.debug.yml",
      "docker-compose.yml"
    ]
  }
}